<div class="container">

<div class="form-group">

<form method="post">

      <input type="hidden" name="cId" value="<%= categoryName.id%>">

    <label>Category Name</label><br>
    <input type="text" name="cname" class="form-control" value="<%= categoryName.name%>" readonly="readonly"><br>

    <label>Parent</label><br>
    <input type="text" name="cparent" class="form-control" value="<%= categoryName.parent%>" readonly="readonly"><br>

                <input type="submit" class="btn btn-success btn-block" value="Delete">

  </form> <br/>

</div>  

</div>

